{
  "name": "yunigreen-frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "predev": "node scripts/dev-guard.mjs",
    "predev:mobile": "node scripts/dev-guard.mjs",
    "predev:admin": "node scripts/dev-guard.mjs",
    "dev": "echo '⚠️  Use dev:mobile or dev:admin to avoid infinite reload issues' && exit 1",
    "dev:mobile": "pnpm --filter @yunigreen/mobile dev",
    "dev:admin": "pnpm --filter @yunigreen/admin dev",
    "dev:all": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test:e2e": "playwright test",
    "test:e2e:admin": "playwright test --project=admin",
    "test:e2e:mobile": "playwright test --project=mobile",
    "test:e2e:ui": "playwright test --ui"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "turbo": "^2.5.4"
  }
}
